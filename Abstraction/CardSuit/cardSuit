package JAVA.OOP.Abstraction.CardSuit;

public enum CardSuit {
    CLUBS(0),
    DIAMONDS(13),
    HEARTS(26),
    SPADES(39);

    private int powerCardSuits;

    CardSuit(int powerCardSuits) {
        this.powerCardSuits = powerCardSuits;
    }

    public int getPowerCardSuits() {
        return powerCardSuits;
    }
}
